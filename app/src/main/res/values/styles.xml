<?xml version="1.0" encoding="utf-8"?>
<resources>
<color name="background">#FFF3E9</color>
    <color name="eggplant">#573353</color>

    <style name="medium">
        <item name="android:fontFamily">@font/manrope_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/eggplant</item>
    </style>

    <style name="bold">
        <item name="android:fontFamily">@font/manrope_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/eggplant</item>
    </style>

    <style name="name">
        <item name="android:fontFamily">@font/manrope_medium</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/eggplant</item>
        <item name="android:alpha">0.5</item>
    </style>

    <style name="num">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/eggplant</item>
    </style>

    <style name="zaglav">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/eggplant</item>
        <item name="android:fontFamily">@font/manrope_bold</item>
    </style>

    <style name="date">
        <item name="android:fontFamily">@font/manrope_medium</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/eggplant</item>
        <item name="android:alpha">0.5</item>
    </style>

    <style name="spinner">
        <item name="android:fontFamily">@font/manrope_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/eggplant</item>
    </style>

    <style name="numred">
        <item name="android:fontFamily">@font/manrope_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#F65B4E</item>
    </style>

    <style name="numyell">
        <item name="android:fontFamily">@font/manrope_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#FDA758</item>
    </style>

    <style name="ActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:colorPrimary">@color/eggplant</item>
    </style>

</resources>